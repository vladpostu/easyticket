{"ast":null,"code":"import { db } from '../../firebase/firebase';\nimport { doc, getDoc, collection } from 'firebase/firestore';\nexport default {\n  name: \"EventoOrganizzatore\",\n  props: [\"eventoId\"],\n  data() {\n    return {\n      evento: null\n    };\n  },\n  methods: {\n    async fetchDatiEvento() {\n      try {\n        const eventoRef = doc(db, \"eventi\", this.eventoId);\n        const snapshot = await getDoc(eventoRef);\n        if (snapshot.exists()) {\n          this.evento = snapshot.data();\n        }\n        const partecipantiRef = collection(db, \"eventi\", this.eventoId, \"partecipanti\");\n        const snapshotPartecipanti = await getDocs(partecipantiRef);\n        if (snapshotPartecipanti.exists()) {\n          console.log(snapshotPartecipanti.docs.data());\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchDatiEvento();\n  }\n};","map":{"version":3,"names":["db","doc","getDoc","collection","name","props","data","evento","methods","fetchDatiEvento","eventoRef","eventoId","snapshot","exists","partecipantiRef","snapshotPartecipanti","getDocs","console","log","docs","error","mounted"],"sources":["/Users/vladpostu/Desktop/easyticket/views/area_riservata/EventoOrganizzatore.vue"],"sourcesContent":["<template>\n    <div v-if=\"evento\" class=\"mt-5\">\n        <h3>{{ evento.nome_evento }}</h3>\n        <div> {{ evento.data }}</div>\n    </div>\n    <div class=\"partecipanti\">\n        \n    </div>\n</template>\n\n<script>\nimport { db } from '../../firebase/firebase';\nimport { doc, getDoc, collection } from 'firebase/firestore';\n\nexport default {\n    name: \"EventoOrganizzatore\",\n    props: [\"eventoId\"],\n    data() {\n        return {\n            evento: null\n        }\n    },\n    methods: {\n        async fetchDatiEvento() {\n            try {\n                const eventoRef = doc(db, \"eventi\", this.eventoId)\n                const snapshot = await getDoc(eventoRef)\n\n                if(snapshot.exists()) {\n                    this.evento = snapshot.data()\n                }\n\n                const partecipantiRef = collection(db, \"eventi\", this.eventoId, \"partecipanti\")\n                const snapshotPartecipanti = await getDocs(partecipantiRef)\n\n                if(snapshotPartecipanti.exists()) {\n                    console.log(snapshotPartecipanti.docs.data())\n                } \n            } catch(error) {\n                console.log(error)\n            }\n        }\n    },\n    mounted() {\n        this.fetchDatiEvento();\n    }\n}\n</script>"],"mappings":"AAWA,SAASA,EAAC,QAAS,yBAAyB;AAC5C,SAASC,GAAG,EAAEC,MAAM,EAAEC,UAAS,QAAS,oBAAoB;AAE5D,eAAe;EACXC,IAAI,EAAE,qBAAqB;EAC3BC,KAAK,EAAE,CAAC,UAAU,CAAC;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,eAAeA,CAAA,EAAG;MACpB,IAAI;QACA,MAAMC,SAAQ,GAAIT,GAAG,CAACD,EAAE,EAAE,QAAQ,EAAE,IAAI,CAACW,QAAQ;QACjD,MAAMC,QAAO,GAAI,MAAMV,MAAM,CAACQ,SAAS;QAEvC,IAAGE,QAAQ,CAACC,MAAM,CAAC,CAAC,EAAE;UAClB,IAAI,CAACN,MAAK,GAAIK,QAAQ,CAACN,IAAI,CAAC;QAChC;QAEA,MAAMQ,eAAc,GAAIX,UAAU,CAACH,EAAE,EAAE,QAAQ,EAAE,IAAI,CAACW,QAAQ,EAAE,cAAc;QAC9E,MAAMI,oBAAmB,GAAI,MAAMC,OAAO,CAACF,eAAe;QAE1D,IAAGC,oBAAoB,CAACF,MAAM,CAAC,CAAC,EAAE;UAC9BI,OAAO,CAACC,GAAG,CAACH,oBAAoB,CAACI,IAAI,CAACb,IAAI,CAAC,CAAC;QAChD;MACJ,EAAE,OAAMc,KAAK,EAAE;QACXH,OAAO,CAACC,GAAG,CAACE,KAAK;MACrB;IACJ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACZ,eAAe,CAAC,CAAC;EAC1B;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}