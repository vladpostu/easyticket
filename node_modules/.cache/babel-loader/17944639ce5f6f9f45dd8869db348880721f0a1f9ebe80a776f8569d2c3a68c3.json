{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport jsQR from 'jsqr';\nexport default {\n  name: \"ApriFotocamera\",\n  data() {\n    return {\n      stream: null,\n      qrContent: \"as\",\n      scanning: false\n    };\n  },\n  async mounted() {},\n  methods: {\n    async apriFotocamera() {\n      try {\n        const stream = await navigator.mediaDevices.getUserMedia({\n          video: {\n            facingMode: \"environment\"\n          }\n        });\n        this.$refs.video.srcObject = stream;\n        this.scanning = true;\n        this.$nextTick(() => {\n          this.scanQR();\n        });\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async scanQR() {\n      const video = this.$refs.video;\n      const canvas = this.$refs.camera;\n      const ctx = canvas.getContext(\"2d\");\n      const scan = () => {\n        if (!this.scanning) return;\n        if (video.readyState === video.HAVE_ENOUGH_DATA) {\n          canvas.width = video.videoWidth;\n          canvas.height = video.height;\n          ctx.drawImage(video, 0, 0, canvas.width, canvas.height);\n          const imageData = ctx.getImageData(0, 0, canvas.width, canvas.height);\n          const code = jsQR(imageData.data, imageData.width, imageData.height);\n          if (code) {\n            this.qrContent = code.data;\n            this.scanning = false;\n            this.stopCamera();\n          }\n          requestAnimationFrame(scan);\n        }\n      };\n      scan();\n    },\n    stopCamera() {\n      if (this.stream) {\n        this.stream.getTracks().forEach(track => track.stop());\n      }\n    }\n  },\n  beforeUnmount() {\n    this.stopCamera();\n  }\n};","map":{"version":3,"names":["jsQR","name","data","stream","qrContent","scanning","mounted","methods","apriFotocamera","navigator","mediaDevices","getUserMedia","video","facingMode","$refs","srcObject","$nextTick","scanQR","erro