ze(35px, 23px);\n\t\t\tbackground:url($flag) no-repeat top right;\n\t\t\tbackground-size:100% auto;\n\t\t\tdisplay:inline-block;\n\t\t}\n\t\t.date, .seats {\n\t\t\tmargin-bottom: 10px;\n\t\t\ttext-transform: uppercase;\n\t\t\tfont-size: .85rem;\n\t\t\tcolor:$lite-big-stone;\n\t\t\tfont-family: $grotesque-regular;\n\t\t}\n\t\t.seats {\n\t\t\tdisplay:inline-block;\n\t\t\tmargin-bottom: 24px;\n\t\t\tpadding-bottom:24px;\n\t\t\tborder-bottom:1px solid $lite-grey;\n\t\t\topacity:0;\n\t\t\ttransition:$timing-2 $transition;\n\t\t}\n\t\t.txt {\n\t\t\tfont-family: $merriweather;\n\t\t\tline-height: 2;\n\t\t\tfont-size: .95rem;\n\t\t\tcolor:$lite-big-stone;\n\t\t\topacity:0;\n\t\t\ttransition:$timing-2 $transition;\n\t\t}\n\t\t.details {\n\t\t\t@include pos(absolute, $left:0, $bottom:0);\n\t\t\tmargin: 10px 0;\n\t\t\tpadding:20px 24px;\n\t\t\tletter-spacing: 1px;\n\t\t\tcolor:$smalt-blue;\n\t\t\tfont-family: $grotesque-black;\n\t\t\tfont-size: .9rem;\n\t\t\ttext-transform: uppercase;\n\t\t\tcursor:pointer;\n\t\t\topacity:0;\n\t\t\ttransition:$timing-2 $transition;\n\t\t}\n\t}\n\t&:hover .infos {\n\t\ttransform:translateY(-$thumb-height);\n\t\t.seats, .txt, .details {\n\t\t\topacity:1;\n\t\t}\n\t}\n}\n</style>\n\n<!-- <div class=\"eventi\">\n        <div>Ultime aggiunte</div>\n        <div class=\"eventi-container\">\n            <div v-for=\"evento in eventi\" :key=\"evento.id\" class=\"evento\">\n                <router-link :to=\"{ name: 'EventoView', params: { eventoId: evento.id } }\">\n                    <div class=\"evento_nome\">\n                        {{  evento.nome_evento }}\n                    </div> \n                    <div class=\"evento_data\">\n                        {{  evento.data }}\n                    </div>\n                </router-link>\n            </div>\n        </div>\n    </div> -->\n\n<script>\nimport { collection, getDocs, query, where } from 'firebase/firestore';\nimport { db } from '../../firebase/firebase';\n\nexport default {\n    name: \"EventiView\",\n    data() {\n        return {\n            eventi: []\n        }\n    },\n    props: [\"organizzatoreId\"],\n    methods: {\n        async fetchEventi() {\n            if (this.organizzatoreId == \"\") {\n                const eventiRef = collection(db, \"eventi\")\n                const snapshot = await getDocs(eventiRef)\n\n                this.eventi = snapshot.docs.map((evento) => ({\n                    id: evento.id,\n                    ...evento.data()\n                }))\n            } else {\n                const q = query(collection(db, \"eventi\"),\n                    where(\"idOrganizzatore\", \"==\", this.organizzatoreId)\n                )\n\n                const snapshot = await getDocs(q);\n                this.eventi = snapshot.docs.map((evento) => ({\n                    id: evento.id,\n                    ...evento.data()\n                }))\n            }\n        }\n    },\n    mounted() {\n        this.fetchEventi();\n    }\n}\n</script>"],"mappings":";;AA2OA,SAASA,UAAU,EAAEC,OAAO,EAAEC,KAAK,EAAEC,KAAI,QAAS,oBAAoB;AACtE,SAASC,EAAC,QAAS,yBAAyB;AAE5C,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,KAAK,EAAE,CAAC,iBAAiB,CAAC;EAC1BC,OAAO,EAAE;IACL,MAAMC,WAAWA,CAAA,EAAG;MAChB,IAAI,IAAI,CAACC,eAAc,IAAK,EAAE,EAAE;QAC5B,MAAMC,SAAQ,GAAIZ,UAAU,CAACI,EAAE,EAAE,QAAQ;QACzC,MAAMS,QAAO,GAAI,MAAMZ,OAAO,CAACW,SAAS;QAExC,IAAI,CAACL,MAAK,GAAIM,QAAQ,CAACC,IAAI,CAACC,GAAG,CAAEC,MAAM,KAAM;UACzCC,EAAE,EAAED,MAAM,CAACC,EAAE;UACb,GAAGD,MAAM,CAACV,IAAI,CAAC;QACnB,CAAC,CAAC;MACN,OAAO;QACH,MAAMY,CAAA,GAAIhB,KAAK,CAACF,UAAU,CAACI,EAAE,EAAE,QAAQ,CAAC,EACpCD,KAAK,CAAC,iBAAiB,EAAE,IAAI,EAAE,IAAI,CAACQ,eAAe,CACvD;QAEA,MAAME,QAAO,GAAI,MAAMZ,OAAO,CAACiB,CAAC,CAAC;QACjC,IAAI,CAACX,MAAK,GAAIM,QAAQ,CAACC,IAAI,CAACC,GAAG,CAAEC,MAAM,KAAM;UACzCC,EAAE,EAAED,MAAM,CAACC,EAAE;UACb,GAAGD,MAAM,CAACV,IAAI,CAAC;QACnB,CAAC,CAAC;MACN;IACJ;EACJ,CAAC;EACDa,OAAOA,CAAA,EAAG;IACN,IAAI,CAACT,WAAW,CAAC,CAAC;EACtB;AACJ","ignoreLi