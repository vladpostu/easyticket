          const imageData = ctx.getImageData(0, 0, canvas.width, canvas.height);\n                    const code = jsQR(imageData.data, imageData.width, imageData.height);\n\n                    if (code) {\n                        this.qrContent = code.data;\n                        this.scanning = false;\n                        this.stopCamera();\n                    }\n                }\n\n                requestAnimationFrame(scan);\n            }\n\n            scan();\n        },\n        stopCamera() {\n            if (this.stream) {\n                this.stream.getTracks().forEach(track => track.stop());\n            }\n        }\n    },\n    beforeUnmount() {\n        this.stopCamera()\n    }\n}\n</script>"],"mappings":";;AAUA,OAAOA,IAAG,MAAO,MAAM;AAEvB,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,IAAI;MACZC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACd;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG,CAEhB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,cAAcA,CAAA,EAAG;MACnB,IAAI;QACA,IAAI,CAACH,QAAO,GAAI,IAAI;QAEpB,IAAI,CAACI,SAAS,CAAC,MAAM;UACjB,IAAI,CAACC,MAAM,CAAC,CAAC;QACjB,CAAC;QAED,IAAI,CAACP,MAAK,GAAI,MAAMQ,SAAS,CAACC,YAAY,CAACC,YAAY,CAAC;UACpDC,KAAK,EAAE;YAACC,UAAU,EAAE;UAAa;QACrC,CAAC,CAAC;QACF,IAAI,CAACC,KAAK,CAACF,KAAK,CAACG,SAAQ,GAAI,IAAI,CAACd,MAAM;MAE5C,EAAE,OAAMe,KAAK,EAAE;QACXC,OAAO,CAACD,KAAK,CAACA,KAAK;MACvB;IACJ,CAAC;IACD,MAAMR,MAAMA,CAAA,EAAG;MACX,MAAMI,KAAI,GAAI,IAAI,CAACE,KAAK,CAACF,KAAK;MAC9B,MAAMM,MAAK,GAAI,IAAI,CAACJ,KAAK,CAACI,MAAM;MAChC,MAAMC,GAAE,GAAID,MAAM,CAACE,UAAU,CAAC,IAAI,CAAC;MAEnC,MAAMC,IAAG,GAAIA,CAAA,KAAM;QACf,IAAG,CAAC,IAAI,CAAClB,QAAQ,EAAE;QAEnB,IAAGS,KAAK,CAACU,UAAS,KAAMV,KAAK,CAACW,gBAAgB,EAAE;UAC5CL,MAAM,CAACM,KAAI,GAAIZ,KAAK,CAACa,UAAU;UAC/BP,MAAM,CAACQ,MAAK,GAAId,KAAK,CAACe,WAAW;UACjCR,GAAG,CAACS,SAAS,CAAChB,KAAK,EAAE,CAAC,EAAE,CAAC,EAAEM,MAAM,CAACM,KAAK,EAAEN,MAAM,CAACQ,MAAM,CAAC;UAEvD,MAAMG,SAAQ,GAAIV,GAAG,CAACW,YAAY,CAAC,CAAC,EAAE,CAAC,EAAEZ,MAAM,CAACM,KAAK,EAAEN,MAAM,CAACQ,MAAM,CAAC;UACrE,MAAMK,IAAG,GAAIjC,IAAI,