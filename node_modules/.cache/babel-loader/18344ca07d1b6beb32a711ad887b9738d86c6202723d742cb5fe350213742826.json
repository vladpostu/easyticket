ottom: 1px solid #e5e7eb;\n  color: #111827;\n}\n\n.table th[scope=\"row\"] {\n  text-align: center;\n  width: 50px; /* larghezza fissa per il numero */\n}\n\n.table td:nth-child(2),\n.table th:nth-child(2) {\n  text-align: left;\n  width: 200px; /* larghezza sufficiente per il nome */\n}\n\n.table td:nth-child(3),\n.table th:nth-child(3) {\n  text-align: left;\n  width: 200px; /* larghezza sufficiente per il cognome */\n}\n\n.table td:nth-child(4),\n.table th:nth-child(4) {\n  text-align: center;\n  width: 150px; /* centrata per la data */\n}\n\n/* Riga alternata */\n.table-striped tbody tr:nth-child(odd) {\n  background-color: #fafbfc;\n}\n\n/* Hover effetto */\n.table tbody tr:hover {\n  background-color: #f1f5f9;\n  transition: background 0.2s ease;\n}\n\n\n</style>\n\n\n\n\n<script>\nimport { db } from '../../firebase/firebase';\nimport { doc, getDoc, collection, getDocs } from 'firebase/firestore';\n\nexport default {\n  name: \"EventoOrganizzatore\",\n  props: [\"eventoId\"],\n  data() {\n    return {\n      evento: null,\n      partecipanti: [],\n      screaming: \"\"\n    }\n  },\n  methods: {\n    async fetchDatiEvento() {\n      try {\n        const eventoRef = doc(db, \"eventi\", this.eventoId)\n        const snapshot = await getDoc(eventoRef)\n\n        if (snapshot.exists()) {\n          this.evento = snapshot.data()\n        }\n\n        const partecipantiRef = collection(db, \"eventi\", this.eventoId, \"partecipanti\")\n        const snapshotPartecipanti = await getDocs(partecipantiRef)\n\n        if (!snapshotPartecipanti.empty) {\n          this.partecipanti = snapshotPartecipanti.docs.map(e => ({\n            id: e.id,\n            ...e.data(),\n          }))\n        } else {\n          this.partecipanti = []\n        }\n      } catch (error) {\n        console.log(error)\n      }\n    }\n  },\n  computed: {\n    partecipantiFiltrati() {\n      if (!this.screaming) return this.partecipanti\n      let search = this.screaming.toLocaleLowerCase();\n      search = search.trim()\n\n      return this.partecipanti.filter((p) => {\n        let fullName = (p.nome.toLocaleLowerCase() + \" \" + p.cognome.toLocaleLowerCase())\n        let fullNameReverse = (p.cognome.toLocaleLowerCase() + \" \" + p.nome.toLocaleLowerCase())\n        return (fullName.includes(search) || fullNameReverse.includes(search))\n      }\n      )\n    }\n  },\n  mounted() {\n    this.fetchDatiEvento();\n  }\n}\n</script>"],"mappings":";;;AAgHA,SAASA,EAAC,QAAS,yBAAyB;AAC5C,SAASC,GAAG,EAAEC,MAAM,EAAEC,UAAU,EAAEC,OAAM,QAAS,oBAAoB;AAErE,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,KAAK,EAAE,CAAC,UAAU,CAAC;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,SAAQ,GAAIZ,GAAG,CAACD,EAAE,EAAE,QAAQ,EAAE,IAAI,CAACc,QAAQ;QACjD,MAAMC,QAAO,GAAI,MAAMb,MAAM,CAACW,SAAS;QAEvC,IAAIE,QAAQ,CAACC,MAAM,CAAC,CAAC,EAAE;UACrB,IAAI,CAACR,MAAK,GAAIO,QAAQ,CAACR,IAAI,CAAC;QAC9B;QAEA,MAAMU,eAAc,GAAId,UAAU,CAACH,EAAE,EAAE,QAAQ,EAAE,IAAI,CAACc,QAAQ,EAAE,cAAc;QAC9E,MAAMI,oBAAmB,GAAI,MAAMd,OAAO,CAACa,eAAe;QAE1D,IAAI,CAACC,oBAAoB,CAACC,KAAK,EAAE;UAC/B,IAAI,CAACV,YAAW,GAAIS,oBAAoB,CAACE,IAAI,CAACC,GAAG,CAACC,CAAA,KAAM;YACtDC,EAAE,EAAED,CAAC,CAACC,EAAE;YACR,GAAGD,CAAC,CAACf,IAAI,CAAC;UACZ,CAAC,CAAC;QACJ,OAAO;UACL,IAAI,CAACE,YAAW,GAAI,EAAC;QACvB;MACF,EAAE,OAAOe,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB;IACF;EACF,CAAC;EACDG,QAAQ,EAAE;IACRC,oBAAoBA,CAAA,EAAG;MACrB,IAAI,CAAC,IAAI,CAAClB,SAAS,EAAE,OAAO,IAAI,CAACD,YAAW;MAC5C,IAAIoB,MAAK,GAAI,IAAI,CAACnB,SAAS,CAACoB,iBAAiB,CAAC,CAAC;MAC/CD,MAAK,GAAIA,MAAM,CAACE,IAAI,CAAC;MAErB,OAAO,IAAI,CAACtB,YAAY,CAACuB,MAAM,CAAEC,CAAC,IAAK;QACrC,IAAIC,QAAO,GAAKD,CAAC,CAACE,IAAI,CAACL,iBAAiB,CAAC,IAAI,GAAE,GAAIG,CAAC,CAACG,OAAO,CAACN,iBAAiB,CAAC,CAAC;QAChF,IAAIO,eAAc,GAAKJ,CAAC,CAACG,OAAO,CAACN,iBAAiB,CAAC,IAAI,GAAE,GAAIG,CAAC,CAACE,IAAI,CAACL,iBAAiB,CAAC,CAAC;QACvF,OAAQI,QAAQ,CAACI,QAAQ,CAACT,MAAM,KAAKQ,eAAe,CAACC,QAAQ,CAACT,MAAM,CAAC;MAC