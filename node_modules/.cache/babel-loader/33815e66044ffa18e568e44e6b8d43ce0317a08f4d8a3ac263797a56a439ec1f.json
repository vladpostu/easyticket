ipt>\nimport { getDoc, doc } from 'firebase/firestore';\nimport { db, auth } from '../../firebase/firebase';\nimport { signInWithEmailAndPassword } from 'firebase/auth';\n\nexport default {\n    name: \"OrganizzatoreLogin\",\n    data() {\n        return {\n            email: \"\",\n            password: \"\"\n        }\n    },\n    methods: {\n        async login() {\n            try {\n                const cred = await signInWithEmailAndPassword(\n                    auth, this.email, this.password\n                )\n                \n                const userDoc = await getDoc(doc(db, \"organizzatori\", cred.user.uid));\n                console.log(userDoc)\n\n                localStorage.setItem(\"organizzatoreId\", cred.user.uid)\n                this.$router.push({name: \"AreaOrganizzatore\"})\n            } catch (error) {\n                console.log(error);\n            }\n        }\n    },\n    mounted() {\n    }\n}\n</script>"],"mappings":";AA8IA,SAASA,MAAM,EAAEC,GAAE,QAAS,oBAAoB;AAChD,SAASC,EAAE,EAAEC,IAAG,QAAS,yBAAyB;AAClD,SAASC,0BAAyB,QAAS,eAAe;AAE1D,eAAe;EACXC,IAAI,EAAE,oBAAoB;EAC1BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,KAAKA,CAAA,EAAG;MACV,IAAI;QACA,MAAMC,IAAG,GAAI,MAAMP,0BAA0B,CACzCD,IAAI,EAAE,IAAI,CAACI,KAAK,EAAE,IAAI,CAACC,QAC3B;QAEA,MAAMI,OAAM,GAAI,MAAMZ,MAAM,CAACC,GAAG,CAACC,EAAE,EAAE,eAAe,EAAES,IAAI,CAACE,IAAI,CAACC,GAAG,CAAC,CAAC;QACrEC,OAAO,CAACC,GAAG,CAACJ,OAAO;QAEnBK,YAAY,CAACC,OAAO,CAAC,iBAAiB,EAAEP,IAAI,CAACE,IAAI,CAACC,GAAG;QACrD,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC;UAACf,IAAI,EAAE;QAAmB,CAAC;MACjD,EAAE,OAAOgB,KAAK,EAAE;QACZN,OAAO,CAACC,GAAG,CAACK,KAAK,CAAC;MACtB;IACJ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG,CACV;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}